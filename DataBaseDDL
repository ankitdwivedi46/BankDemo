CREATE TABLE CUSTOMER_DATA(
CUSTOMER_NAME VARCHAR(50) NOT NULL,
CUSTOMER_MOBILE VARCHAR(10) NOT NULL UNIQUE,
CUSTOMER_EMAIL VARCHAR(50) NOT NULL,
CUSTOMER_DOB VARCHAR(10) NOT NULL,
CUSTOMER_AADHAR VARCHAR(12) NOT NULL UNIQUE,
CUSTOMER_PAN VARCHAR(10) NOT NULL UNIQUE,
CUSTOMER_ACC_NO VARCHAR(32) NOT NULL PRIMARY KEY,
CUSTOMER_IFSC VARCHAR(5) DEFAULT '12345',
CUSTOMER_ADDRESS VARCHAR(50) NOT NULL)

SELECT * FROM CUSTOMER_DATA

CREATE TABLE CUSTOMER_ACCOUNT(
CUSTOMER_ACC_NO VARCHAR(32) NOT NULL PRIMARY KEY
,CUSTOMER_BALANCE VARCHAR(50) )

SELECT * FROM CUSTOMER_ACCOUNT




CREATE TABLE
CUSTOMER_CREDENTIAL_DATABANK(
CUSTOMER_ACC_NO VARCHAR(32) PRIMARY KEY,
CUSTOMER_USERNAME VARCHAR(8) NOT NULL UNIQUE,
CUSTOMER_PASSWORD VARCHAR(150) NOT NULL,
CUSTOMER_PIN CHAR(4) NOT NULL,
CUSTOMER_FIRST_LOGIN_FLAG CHAR(1) NOT NULL,
DECRYPT_KEY VARCHAR(150) NOT NULL)

SELECT * FROM CUSTOMER_CREDENTIAL_DATABANK